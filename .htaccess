
RewriteEngine On
RewriteRule ^$ /container/acceso.php [L,QSA]
RewriteRule ^registrar$ /container/registrar.php [L,QSA]
RewriteRule ^([^/]+)$ /config/priv.php [L,QSA]
RewriteRule ^([^/]+)/$ /config/priv.php [L,QSA]
RewriteRule ^container/([^/]+)/$ /config/priv.php [L,QSA]

#dash_admin
RewriteRule ^admin/panel$ /container/dash_admin/panel.php [L,QSA]
RewriteRule ^admin/recargos$ /container/dash_admin/recargos.php [L,QSA]
RewriteRule ^admin/clientes$ /container/dash_admin/clientes.php [L,QSA]


#dash_facturas
RewriteRule ^facturas/recargos/([^/]+)$ /container/dash_facturas/factura_recargo.php?referencia=$1 [L,QSA]

#mi
RewriteRule ^mi/acceso$ /config/acceso.php [L,QSA]
RewriteRule ^mi/cerrar_session$ /config/cerrar_session.php [L,QSA]
RewriteRule ^mi/res_pass$ /src/res_pass.php [L,QSA]
RewriteRule ^mi/phpmailer_ress$ /src/phpmailer_ress.php [L,QSA]

RewriteRule ^mi/src/clientes$ /src/clientes.php [L,QSA]
RewriteRule ^mi/src/consultar_cliente$ /src/consultar_cliente.php [L,QSA]
RewriteRule ^mi/src/editar_cliente$ /src/editar_cliente.php [L,QSA]
RewriteRule ^mi/src/guardar_cliente$ /src/guardar_cliente.php [L,QSA]
RewriteRule ^mi/src/eliminar_cliente$ /src/eliminar_cliente.php [L,QSA]
RewriteRule ^mi/src/recargos$ /src/recargos.php [L,QSA]

